{
  "name": "portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "node -r dotenv/config ./node_modules/.bin/ng",
    "start": "node -r dotenv/config ./node_modules/.bin/ng serve",
    "build": "node -r dotenv/config ./node_modules/.bin/ng build",
    "postbuild": "gzipper compress dist/portfolio",
    "watch": "node -r dotenv/config ./node_modules/.bin/ng build --watch --configuration development",
    "test": "node -r dotenv/config ./node_modules/.bin/ng test",
    "serve:ssr:Portfolio": "node dist/portfolio/server/server.mjs",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix lint",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.2",
    "@angular/common": "^20.1.2",
    "@angular/compiler": "^20.1.2",
    "@angular/core": "^20.1.2",
    "@angular/forms": "^20.1.2",
    "@angular/platform-browser": "^20.1.2",
    "@angular/platform-browser-dynamic": "^20.1.2",
    "@angular/platform-server": "^20.1.2",
    "@angular/router": "^20.1.2",
    "@angular/ssr": "^20.1.1",
    "@ng-icons/core": "^30.2.0",
    "@ng-icons/heroicons": "^30.2.0",
    "@ng-icons/material-icons": "^30.2.0",
    "@ng-icons/svgl": "^30.2.0",
    "@primeuix/themes": "^1.2.1",
    "@tailwindcss/postcss": "^4.0.4",
    "@vercel/analytics": "^1.4.1",
    "@vercel/speed-insights": "^1.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "gzipper": "^8.2.0",
    "postcss": "^8.5.1",
    "primeng": "^20.0.0",
    "rxjs": "~7.8.1",
    "tailwindcss": "^4.0.4",
    "tailwindcss-primeui": "^0.4.0",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.1",
    "@angular/cli": "^20.1.1",
    "@angular/compiler-cli": "^20.1.2",
    "@types/express": "^5.0.0",
    "@types/jasmine": "~5.1.5",
    "@types/node": "^22.13.1",
    "angular-eslint": "^20.1.1",
    "eslint": "^9.19.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "prettier": "^3.4.2",
    "pretty-quick": "^4.0.0",
    "typescript": "~5.8.3"
  }
}
